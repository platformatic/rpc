{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "lib": ["es2022"],
    "target": "esnext",
    "sourceMap": true,
    "pretty": true,
    "noEmitOnError": true,
    "incremental": true,
    "strict": true,
    "outDir": "dist",
    "skipLibCheck": true
  }
}

